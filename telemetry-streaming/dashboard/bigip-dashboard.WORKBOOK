      {
        "type": 9,
        "content": {
          "version": "KqlParameterItem/1.0",
          "crossComponentResources": [
            "{Workspace}"
          ],
          "parameters": [
            {
              "id": "a6108e0b-1ef1-4f84-b95f-4826e8861ec0",
              "version": "KqlParameterItem/1.0",
              "name": "parmFqdnDomain",
              "label": "Domain",
              "type": 2,
              "query": "F5Telemetry_aWideIps_CL\r\n| where TimeGenerated > ago(2m) \r\n| extend wip = tostring(split(name_s, '/')[-1])\r\n| extend domain_s = substring(tostring(split(name_s, '/')[-1]),indexof(wip,\".\",0)+1,strlen(wip))\r\n| distinct domain_s",
              "crossComponentResources": [
                "{Workspace}"
              ],
              "typeSettings": {
                "additionalResourceOptions": [],
                "showDefault": false
              },
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces",
              "value": null
            }
          ],
          "style": "pills",
          "queryType": 0,
          "resourceType": "microsoft.operationalinsights/workspaces"
        },
        "name": "parameters - 6 - Copy"
      },
      {
        "type": 3,
        "content": {
          "version": "KqlItem/1.0",
          "query": "F5Telemetry_aWideIps_CL\r\n| union F5Telemetry_aaaaWideIps_CL\r\n| where TimeGenerated > ago(2m)\r\n| where _ResourceId endswith '{ReportingDNSSystem}'\r\n| extend UniqueRecordIdentifier = strcat(_ResourceId, \"_\", wipType_s, \"_\", name_s)\r\n| summarize arg_max(TimeGenerated, *) by UniqueRecordIdentifier\r\n| extend fqdn = tostring(split(name_s, '/')[-1])\r\n| where '{parmFqdnDomain}' == '' or name_s endswith '{parmFqdnDomain}' \r\n| project \r\n    Service = strcat(tostring(split(fqdn, '.')[0]),' (',wipType_s,')'),\r\n    ['Full FQDN'] = fqdn,\r\n    Environment = toupper(split(fqdn, '.')[1]),\r\n    Domain = toupper(split(fqdn, '.')[2]),\r\n    Type = wipType_s,\r\n    Partition = strcat(partition_s,\"/\",subPath_s),\r\n    Status = strcat(status_availabilityState_s,\" (\",status_enabledState_s,\")\"),\r\n    Reason = status_statusReason_s,\r\n    TotalRequests = requests_d,\r\n    ResolvedRequests = resolutions_d,\r\n    Pools = pools_s,\r\n    UniqueRecordIdentifier\r\n",
          "size": 3,
          "title": "GSLB - Global Availability",
          "timeContext": {
            "durationMs": 86400000
          },
          "showRefreshButton": true,
          "exportedParameters": [
            {
              "fieldName": "Pools",
              "parameterName": "parmWipPools",
              "defaultValue": "null"
            },
            {
              "fieldName": "UniqueRecordIdentifier",
              "parameterName": "parmURN",
              "parameterType": 1,
              "defaultValue": "null"
            }
          ],
          "queryType": 0,
          "resourceType": "microsoft.operationalinsights/workspaces",
          "crossComponentResources": [
            "{Workspace}"
          ],
          "visualization": "tiles",
          "tileSettings": {
            "titleContent": {
              "columnMatch": "Service",
              "formatter": 7,
              "formatOptions": {
                "linkTarget": "GenericDetails",
                "linkIsContextBlade": true
              },
              "numberFormat": {
                "unit": 0,
