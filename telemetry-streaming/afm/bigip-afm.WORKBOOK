          "showLegend": true
        }
      },
      "customWidth": "50",
      "name": "queryRuleHits",
      "styleSettings": {
        "maxWidth": "50"
      }
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "F5Telemetry_AFM_CL \r\n| where isnotempty(acl_policy_type_s)\r\n| where iff(isnotempty('{q15brush}'),TimeGenerated {q15brush},TimeGenerated {parmTimeRange})\r\n| where iff({parmHideAzure},source_ip_s !startswith \"40.80\" and source_ip_s !startswith \"40.87\" , true)\r\n| where '{parmPolicy}' == '' or acl_policy_name_s contains '{parmPolicy}'\r\n| where '{parmRuleHits}' == 'null' or acl_rule_name_s contains todynamic('{parmRuleHits}').label\r\n| summarize Count = count() by ['Source IP'] = source_ip_s\r\n| order by Count\r\n| take 10",
        "size": 0,
