            "id": "656e6cd5-ca1f-42c9-b327-f1777a8f3148",
            "version": "KqlParameterItem/1.0",
            "name": "parmHideAzure",
            "label": "Hide Azure Monitor IPs",
            "type": 10,
            "description": "This will hide all logs for connections inbound from Azure Monitor ranges 40.80.0.0/16 and 40.87.0.0/16",
            "isRequired": true,
            "typeSettings": {
